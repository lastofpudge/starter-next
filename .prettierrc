{
  "trailingComma": "none",
  "printWidth": 100,
  "bracketSameLine": true,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@pages/(.*)$",
    "^@components/(.*)$",
    "^@providers/(.*)$",
    "^@services/(.*)$",
    "^@utils/(.*)$",
    "^@types/(.*)$",
    "^@hooks/(.*)$",
    "^@store/(.*)$",
    "^@styles/(.*)$",
    "^@assets/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}